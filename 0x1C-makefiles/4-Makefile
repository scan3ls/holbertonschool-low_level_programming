CC = gcc
CFLAGS = -Wall -Werror -Wextra -pedantic
NAME = holberton
SRC = main.c holberton.c
OBJ = main.o holberton.o
RM = rm -f

all: $(OBJ)
		$(CC) $(CFLAGS) -c -o main.o main.c
		$(CC) $(CFLAGS) -c -o holberton.o holberton.c
		$(CC) $(OBJ) -o $(NAME)

clean:
		$(RM) *~ $(NAME)

oclean:
		$(RM) $(OBJ)

fclean: clean oclean

re: oclean all
		